{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
      "quotemark": [true, "single"],
      "semicolon": [true, "never"],
      "indent": [true, "spaces", 4],
      "import-spacing": true,
      "ordered-imports": [
          true,
          {
              "grouped-imports": true
          }
      ]
  },
  "rulesDirectory": []
}